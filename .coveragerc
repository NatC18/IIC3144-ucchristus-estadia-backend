[run]
source = api
omit =
    */migrations/*
    */tests/*
    */models/*
    */seeds/*
    */admin.py
    */apps.py
    manage.py
    */wsgi.py
    */asgi.py
    */settings.py
    */episodio_local_backup.py

[report]
exclude_lines =
    pragma: no cover
    def __str__
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError: